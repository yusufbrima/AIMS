   "source": [
    "import numpy as np"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 8,
   "metadata": {},
   "outputs": [],
   "source": [
    "def self_attention(K,Q,V):\n",
    "    A  = np.exp(K@Q.T)/np.sqrt(K.shape[1])\n",
    "    return (A  /  np.sum(A,1))@V"
   ]
  },
  {
